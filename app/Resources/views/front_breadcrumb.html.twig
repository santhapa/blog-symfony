{% if wo_breadcrumbs()|length %}
  {% spaceless %}
    {% for b in breadcrumbs %}
        {# {% if b.url and not loop.last %} #}
        {% if b.url %}
            <a href="{{ b.url }}">{{- b.text -}}</a>
        {% else %}
            <span>{{- b.text -}}</span>
        {% endif %}
        {% if not loop.last %}/{% endif %}
    {% endfor %}
  {% endspaceless %}
{% endif %}
